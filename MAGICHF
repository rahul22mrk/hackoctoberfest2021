// https://www.codechef.com/problems/MAGICHF

#include <iostream>

using namespace std;

int main()
{
    int t,n,x,s,a,b;
    cin>>t;
    for(int j=1;j<=t;j++)
    {
        cin>>n>>x>>s;
        for(int i=1;i<=s;i++)
        {
            cin>>a>>b;
            if(x==a)
                x=b;
            else if(x==b)
                x=a;

        }
        cout<<x<<endl;
    }


    return 0;
}
